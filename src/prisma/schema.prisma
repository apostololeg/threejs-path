datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  binaryTargets = ["debian-openssl-3.0.x", "debian-openssl-1.1.x"]
}

model User {
  id String @id // username
  joinedAt DateTime @default(now())
  password String
  position Float[] @default([0,0,0])
}
